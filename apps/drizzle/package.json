{
  "name": "@apps/drizzle",
  "version": "1.0.0",
  "description": "Drizzle ORM implementation",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write \"src/**/*.{ts,json}\"",
    "test": "jest",
    "test:unit": "jest --config jest.config.js",
    "test:integration": "jest --config jest.integration.config.js",
    "clean": "rm -rf dist",
    "generate": "drizzle-kit generate:pg",
    "migrate": "tsx src/migrate.ts",
    "seed": "tsx src/seeds/seed.ts"
  },
  "dependencies": {
    "@packages/common": "workspace:*",
    "@packages/config": "workspace:*",
    "@packages/constants": "workspace:*",
    "@packages/logger": "workspace:*",
    "@packages/swagger": "workspace:*",
    "@packages/types": "workspace:*",
    "@packages/validation": "workspace:*",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "drizzle-kit": "^0.20.0",
    "drizzle-orm": "^0.29.0",
    "express": "^4.18.2",
    "helmet": "^7.0.0",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.11.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/node": "^20.4.2",
    "@types/pg": "^8.10.2",
    "jest": "^29.6.1",
    "supertest": "^6.3.3",
    "ts-jest": "^29.4.6",
    "tsx": "^3.12.7",
    "typescript": "^5.1.6"
  },
  "keywords": [
    "nodejs",
    "typescript",
    "express",
    "drizzle",
    "postgresql"
  ],
  "author": "Your Name",
  "license": "MIT"
}
