{
  "name": "@apps/sequelize",
  "version": "1.0.0",
  "description": "Sequelize ORM implementation",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write \"src/**/*.{ts,json}\"",
    "test": "jest",
    "test:unit": "jest --config jest.config.js",
    "test:integration": "jest --config jest.integration.config.js",
    "clean": "rm -rf dist",
    "migration:generate": "npx sequelize-cli migration:generate",
    "migration:run": "npx sequelize-cli db:migrate",
    "migration:revert": "npx sequelize-cli db:migrate:undo",
    "seed": "tsx src/seeds/seed.ts"
  },
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.0.0",
    "dotenv": "^16.3.1",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "sequelize": "^6.32.1",
    "pg": "^8.11.0",
    "pg-hstore": "^2.3.4",
    "@packages/config": "workspace:*",
    "@packages/constants": "workspace:*",
    "@packages/logger": "workspace:*",
    "@packages/common": "workspace:*",
    "@packages/swagger": "workspace:*",
    "@packages/types": "workspace:*",
    "@packages/validation": "workspace:*"
  },
  "devDependencies": {
    "sequelize-cli": "^6.6.1",
    "@types/express": "^4.17.17",
    "@types/node": "^20.4.2",
    "@types/bcryptjs": "^2.4.2",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/cors": "^2.8.13",
    "tsx": "^3.12.7",
    "typescript": "^5.1.6",
    "@types/jest": "^29.5.3",
    "jest": "^29.6.1",
    "supertest": "^6.3.3"
  },
  "keywords": [
    "nodejs",
    "typescript",
    "express",
    "sequelize",
    "postgresql"
  ],
  "author": "Your Name",
  "license": "MIT"
}
